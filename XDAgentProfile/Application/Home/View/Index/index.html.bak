   <script>
      show_num(<{$countnum}>);
      $(function(){
         getdata();
         setInterval('getdata()', 3000);
      });
      function getdata(){
         $.ajax({
             url: '__ROOT__/index',
            type: 'POST',
            dataType: "json",
            data:{'option':'counting'},
            cache: false,
            timeout: 10000,
            error: function(){
               show_num(<{$countnum}>);
            },
            success: function(data){
               show_num(data.countnum);
             }
            });
      }
      function show_num(n){
         var it = $(".t_num i");
         var len = String(n).length;
         for(var i=0;i<len;i++){
            if(it.length<=i){
               $(".t_num").append("<i></i>");
            }
            var num=String(n).charAt(i);
            var y = -parseInt(num)*73.2;
            var obj = $(".t_num i").eq(i);
            obj.animate({
               backgroundPosition :'(0 '+String(y)+'px)' 
               }, 'slow','swing',function(){}
            );
         }
         $("#cur_num").val(n);
      }
   </script>

